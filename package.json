  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@wordpress/scripts": "^20.0.2"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "tailwindcss": "^2.0"
  },
  "peerDependencies": {
    "cssnano": "^5.0.7"
  }
}
